<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_orange_edit_press" />
            <corners android:radius="10dp" />
            <stroke android:width="2dp" android:color="@color/color_red_orange_background" />
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_orange_edit_text" />
            <corners android:radius="10dp" />
            <stroke android:width="2dp" android:color="@color/color_orange_background" />
        </shape>
    </item>
</selector>